// Java code illustrating addAll() 
import java.util.*; 
class Vector_demo { 
	public static void main(String[] arg) 
	{ 
		ArrayList arr = new ArrayList(); 
		arr.add(3); 
		arr.add("Welcome"); 
		arr.add("Welcome"); 
		arr.add(4); 

		// createn default vector 
		Vector v = new Vector(); 

		// copying all element of array list int0 vector 
		v.addAll(arr); 

		// checking vector v 
		System.out.println("vector v:" + v); 
	} 
}
